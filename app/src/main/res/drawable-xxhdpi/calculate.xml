<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="13dip" />
            <stroke android:width="1dip" android:color="#dedede" />
            <gradient android:angle="-90" android:startColor="#dedede" android:endColor="#939393"  />
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="13dip" />
            <stroke android:width="1dip" android:color="#dedede" />
            <gradient android:angle="-90" android:startColor="#ffaae3e8" android:endColor="#ff49b4ee" />
        </shape>
    </item>
</selector>